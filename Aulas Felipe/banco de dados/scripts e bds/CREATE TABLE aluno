CREATE TABLE aluno(
    --PRMARY KEY : é a chave primária da tabela!
    --AUTOINCREMENT: utilizado para somar +1 número automaticamente
    --NOT NULL : é para deixar que o campo tenha valor vazil ou null
    ra INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    rg TEXT NOT NULL,
    escolaridade TEXT NOT NULL,
    email TEXT NOT NULL,
    nome TEXT NOT NULL,                              
    endereço TEXT NOT NULL
   );